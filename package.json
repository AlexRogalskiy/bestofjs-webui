{
  "name": "bestofjs-webui",
  "version": "0.11.3",
  "description": "bestof.js.org front-end web application",
  "engines": {
    "node": ">=8.1.2"
  },
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "test": "cross-env NODE_ENV=test babel-tape-runner test/**/*Spec.js | faucet",
    "test-debug": "cross-env NODE_ENV=test babel-tape-runner test/**/AboutSpec.js",
    "clean": "rimraf www/index.html && rimraf www/build",
    "build": "npm run build-html && npm run build-js",
    "start": "cross-env NODE_ENV=development node config/devserver.js",
    "build-js": "cross-env NODE_ENV=production webpack -p --config config/webpack.production.config.js --progress",
    "gh-pages": "node scripts/deploy-gh-pages/",
    "surge": "surge --domain bestofjs.surge.sh --project ./www/",
    "deploy-all": "npm run build && npm run gh-pages",
    "daily": "npm run build-html && npm run build-html-hof && npm run rss-once-a-week && npm run commit-ssr-files",
    "build-html": "cross-env NODE_ENV=production babel-node scripts/build/",
    "build-html-hof": "cross-env NODE_ENV=production babel-node scripts/build/build-hof.js",
    "commit-ssr-files": "node scripts/commit-ssr-files",
    "analyze": "cross-env NODE_ENV=production webpack -p --config config/webpack.production.config.js --json | webpack-bundle-size-analyzer",
    "rss-once-a-week": "cross-env NODE_ENV=production babel-node scripts/rss/once-a-week",
    "rss": "babel-node scripts/rss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestofjs/bestofjs-webui"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bestofjs/bestofjs-webui/issues"
  },
  "homepage": "https://bestof.js.org",
  "dependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.3",
    "classnames": "^2.2.5",
    "cross-env": "^1.0.7",
    "css-loader": "0.28.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.11.1",
    "formik": "0.11.11",
    "fs-extra": "3.0.1",
    "html-minifier": "3.4.3",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.intersection": "^4.4.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.times": "^4.3.2",
    "marked": "^0.3.5",
    "node-fetch": "^1.3.3",
    "notie": "^2.1.0",
    "numeral": "^1.5.3",
    "pify": "^2.3.0",
    "postcss-loader": "1.3.3",
    "preact": "8.1.0",
    "preact-compat": "3.15.0",
    "prop-types": "15.5.8",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-calendar-heatmap": "1.4.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "1.0.0-rc.10",
    "react-toggled": "^1.0.2",
    "recursive-readdir": "^2.1.1",
    "redux": "3.6.0",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^1.0.0",
    "reselect": "^3.0.0",
    "slideout": "^0.1.11",
    "style-loader": "0.16.1",
    "styled-components": "^3.2.6",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "tinytime": "^0.2.6",
    "url-loader": "0.5.8",
    "vague-time": "^1.3.0",
    "webpack": "^3.7.1",
    "xmlbuilder": "^9.0.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-register": "6.5.2",
    "babel-tape-runner": "2.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "3.14.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^7.1.0",
    "faucet": "0.0.1",
    "gh-pages": "0.10.0",
    "jsdom": "^7.0.2",
    "prettier": "^1.5.2",
    "pretty-bytes": "^4.0.2",
    "react-test-renderer": "^16.0.0",
    "sinon": "^1.17.3",
    "tape": "^4.2.2",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-monitor": "^1.0.13"
  }
}
