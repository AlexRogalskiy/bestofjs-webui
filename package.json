{
  "name": "bestofjs-webui",
  "version": "0.8.0",
  "description": "bestof.js.org front-end web application",
  "engines": {
    "node": ">=6.2.1"
  },
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "test": "cross-env NODE_ENV=test babel-tape-runner test/**/*Spec.js | faucet",
    "test-debug": "babel-tape-runner test/**/ProjectListSpec.js",
    "clean": "rimraf www/index.html && rimraf www/build",
    "build": "npm run build-html && npm run build-js",
    "start": "cross-env NODE_ENV=development node config/devserver.js",
    "build-js": "cross-env NODE_ENV=production webpack -p --config config/webpack.production.config.js --progress",
    "gh-pages": "node scripts/deploy-gh-pages/",
    "surge": "surge --domain bestofjs.surge.sh --project ./www/",
    "deploy-all": "npm run build && npm run gh-pages",
    "daily": "npm run build-html && npm run build-html-hof && npm run deploy-html",
    "build-html": "babel-node scripts/build/",
    "build-html-hof": "babel-node scripts/build/build-hof.js",
    "deploy-html": "node scripts/deploy-html/",
    "analyze": "cross-env NODE_ENV=production webpack -p --config config/webpack.production.config.js --json | webpack-bundle-size-analyzer",
    "rss": "babel-node scripts/rss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelrambeau/bestofjs-webui.git"
  },
  "keywords": ["react", "redux", "webpack"],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelrambeau/bestofjs-webui/issues"
  },
  "homepage": "https://bestof.js.org",
  "dependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "cross-env": "^1.0.7",
    "css-loader": "0.28.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.0.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-minifier": "3.4.3",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.intersection": "^4.4.0",
    "lodash.mapvalues": "^4.6.0",
    "marked": "^0.3.5",
    "node-fetch": "^1.3.3",
    "notie": "^2.1.0",
    "numeral": "^1.5.3",
    "pify": "^2.3.0",
    "postcss-loader": "1.3.3",
    "preact": "8.1.0",
    "preact-compat": "3.15.0",
    "prop-types": "15.5.8",
    "react": "15.6.0",
    "react-dom": "15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "5.0.4",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-select": "1.0.0-beta14",
    "recursive-readdir": "^2.1.1",
    "redux": "3.6.0",
    "redux-form": "6.6.3",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^1.0.0",
    "reselect": "^3.0.0",
    "slideout": "^0.1.11",
    "style-loader": "0.16.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.8",
    "vague-time": "^1.3.0",
    "webpack": "2.4.1",
    "xmlbuilder": "^9.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-register": "6.5.2",
    "babel-tape-runner": "2.0.1",
    "enzyme": "^2.8.0",
    "eslint": "3.14.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^7.1.0",
    "faucet": "0.0.1",
    "gh-pages": "0.10.0",
    "jsdom": "^7.0.2",
    "prettier": "^1.5.2",
    "react-addons-perf": "15.0.1",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.5.4",
    "sinon": "^1.17.3",
    "tape": "^4.2.2",
    "webpack-dev-server": "2.4.5",
    "why-did-you-update": "0.0.8"
  }
}
