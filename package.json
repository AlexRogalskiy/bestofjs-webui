{
  "name": "bestofjs-webui",
  "version": "0.2.1",
  "description": "bestof.js.org front-end web application",
  "main": "webpack.local.config.js",
  "scripts": {
    "test": "babel-tape-runner test/**/*.js | faucet",
    "test-debug": "babel-tape-runner test/**/*.js",
    "start": "webpack-dev-server --config config/webpack.local.config.js --content-base www --hot",
    "build": "webpack -p --config config/webpack.production.config.js",
    "gh-pages": "node config/gh-pages-deploy",
    "deploy": "npm run build && npm run gh-pages",
    "minify": "html-minifier --remove-comments --minify-css src/index.html > www/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelrambeau/bestofjs-webui.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Michael Rambeau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelrambeau/bestofjs-webui/issues"
  },
  "homepage": "http:bestof.js.org",
  "dependencies": {
    "axios": "^0.7.0",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.12.0",
    "es6-promise": "^3.0.2",
    "history": "^1.9.0",
    "html-minifier": "^1.0.0",
    "lodash": "^3.8.0",
    "moment": "^2.10.6",
    "numeral": "^1.5.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^2.1.2",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.2",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^0.1.0",
    "style-loader": "^0.12.1",
    "stylus-loader": "^1.2.0",
    "webpack": "^1.8.11"
  },
  "devDependencies": {
    "babel-tape-runner": "^1.2.0",
    "faucet": "0.0.1",
    "gh-pages": "^0.4.0",
    "jsdom": "^7.0.2",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.7",
    "tape": "^4.2.2",
    "webpack-dev-server": "^1.9.0"
  }
}
