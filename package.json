{
  "name": "bestofjs-webui",
  "version": "0.3.0",
  "description": "bestof.js.org front-end web application",
  "main": "webpack.local.config.js",
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "babel-tape-runner test/**/*Spec.js | faucet",
    "test-debug": "babel-tape-runner test/**/ProjectSpec.js",
    "clean": "rimraf www/index.html && rimraf www/build",
    "build": "npm run build-html && npm run build-js",
    "start": "webpack-dev-server --config config/webpack.local.config.js --content-base www/ --hot",
    "build-js": "webpack -p --config config/webpack.production.config.js",
    "gh-pages": "node scripts/deploy-gh-pages/",
    "firebase": "firebase deploy",
    "deploy-all": "npm run build && npm run gh-pages",
    "daily": "npm run build-html && npm run deploy-html",
    "build-html": "babel-node scripts/build/",
    "deploy-html": "babel-node scripts/deploy-html/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelrambeau/bestofjs-webui.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Michael Rambeau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelrambeau/bestofjs-webui/issues"
  },
  "homepage": "http:bestof.js.org",
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "axios": "^0.7.0",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-react-display-name": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "css-loader": "^0.12.0",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.26.5",
    "history": "2.0.0",
    "html-minifier": "^1.0.0",
    "html-webpack-plugin": "^2.9.0",
    "lodash": "^3.8.0",
    "marked": "^0.3.5",
    "node-fetch": "^1.3.3",
    "numeral": "^1.5.3",
    "postcss-loader": "^0.8.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "react-router-redux": "3.0.0",
    "react-select": "^1.0.0-beta8",
    "redux": "^3.0.4",
    "redux-form": "^4.0.5",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^1.0.0",
    "scroll-behavior": "0.3.2",
    "slideout": "^0.1.11",
    "style-loader": "^0.12.1",
    "stylus-loader": "^1.2.0",
    "vague-time": "^1.3.0",
    "webpack": "^1.8.11"
  },
  "devDependencies": {
    "babel-register": "6.5.2",
    "babel-tape-runner": "2.0.1",
    "enzyme": "1.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "faucet": "0.0.1",
    "gh-pages": "0.10.0",
    "jsdom": "^7.0.2",
    "react-addons-test-utils": "0.14.7",
    "react-hot-loader": "^1.2.7",
    "tape": "^4.2.2",
    "webpack-dev-server": "^1.9.0"
  }
}
